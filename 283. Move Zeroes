public class Solution {
    public void moveZeroes(int[] nums) {
    
    
    if (nums == null || nums.length == 0) return;   
        int temp=0;
        for(int i=0;i<nums.length;i++)
        {
            for(int j=i;j<nums.length;j++)
            {
                if(nums[i]==0&&nums[j]!=0)
                {
                temp=nums[j];
                nums[j]=nums[i];
                nums[i]=temp;
                }
            }
        }
        }
        }
        
        
        
